{
  "name": "newmeca-v2",
  "version": "1.0.0",
  "private": true,
  "description": "MECA V2 - Car Audio Competition Management Platform",
  "scripts": {
    "dev": "rush dev -t @newmeca/frontend",
    "dev:backend": "rush dev -t @newmeca/backend",
    "dev:all": "npm run supabase:start && concurrently \"rush dev -t @newmeca/backend\" \"rush dev -t @newmeca/frontend\"",
    "build": "rush build",
    "build:frontend": "rush build -t @newmeca/frontend",
    "build:backend": "rush build -t @newmeca/backend",
    "typecheck": "rush typecheck",
    "lint": "rush lint",
    "install:all": "rush update",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:restart": "npx supabase stop && npx supabase start"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "pg": "^8.16.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "node-fetch": "^2.7.0"
  }
}
